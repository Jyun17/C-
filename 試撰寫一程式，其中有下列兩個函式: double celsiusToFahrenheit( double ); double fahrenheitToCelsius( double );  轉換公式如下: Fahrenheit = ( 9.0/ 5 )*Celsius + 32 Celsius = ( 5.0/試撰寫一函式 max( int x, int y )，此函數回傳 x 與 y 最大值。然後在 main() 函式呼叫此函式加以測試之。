#include <iostream>
using namespace std;

int max(int x, int y) {
    return (x > y) ? x : y;
}

int main() {
    int a, b;

   
    cout << "請輸入第一個整數: ";
    cin >> a;

    
    cout << "請輸入第二個整數: ";
    cin >> b;

    
    cout << a << " 與 " << b << " 的最大值是 " << max(a, b) << std::endl;

    return 0;
}
